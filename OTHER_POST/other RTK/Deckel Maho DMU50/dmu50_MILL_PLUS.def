   	}
   	ADDRESS K2Y
  	{
      		FORMAT      			Coordinate
      		FORCE       			off
      		MAX         				99999.9999 Warning
      		MIN         				-99999.9999 Warning
      		ZERO_FORMAT 		Zero_real
      		LEADER 				"Y52="
   	}
   	ADDRESS K1Y
  	{
      		FORMAT      			Coordinate
      		FORCE       			off
      		MAX         				99999.9999 Warning
      		MIN         				-99999.9999 Warning
      		ZERO_FORMAT 		Zero_real
      		LEADER 				"Y53="
   	}
   	ADDRESS K3Z
  	{
      		FORMAT      			Coordinate
      		FORCE       			off
      		MAX         				99999.9999 Warning
      		MIN         				-99999.9999 Warning
      		ZERO_FORMAT 		Zero_real
      		LEADER 				"Z51="
   	}
   	ADDRESS K2Z
  	{
      		FORMAT      			Coordinate
      		FORCE       			off
      		MAX         				99999.9999 Warning
      		MIN         				-99999.9999 Warning
      		ZERO_FORMAT 		Zero_real
      		LEADER 				"Z52="
   	}
   	ADDRESS K1Z
  	{
      		FORMAT      			Coordinate
      		FORCE       			off
      		MAX         				99999.9999 Warning
      		MIN         				-99999.9999 Warning
      		ZERO_FORMAT 		Zero_real
      		LEADER 				"Z53="
   	}
   	ADDRESS G_cutcom 
  	{
      		FORMAT      			G_command
      		FORCE       			off
      		LEADER      			"G"
  	}
  	ADDRESS G_feed 
  	{
      		FORMAT      			G_command
      		FORCE       			off
      		LEADER      			"G"
  	}
	ADDRESS G_mode 
	{
		FORMAT				G_command
		FORCE       			off
		LEADER      			"G"
	}
	ADDRESS G_plane 
	{
		FORMAT      			G_command
		FORCE       			off
		LEADER      			"G"
	}
	ADDRESS G_dwell
	{
		FORMAT      			G_command
		FORCE       			off
		LEADER      			"G"
	}
	ADDRESS G
	{
		FORMAT      			G_command
		FORCE       			off
		LEADER      			"G"
	}
	ADDRESS Radius
	{
		FORMAT      			Coordinate
      		FORCE       			Always
      		MAX         				99999.999 Warning
      		MIN         				0.0 Warning
      		ZERO_FORMAT 		Zero_real
      		LEADER 				"R"
	}
	 ADDRESS K_cycle 
	{
		FORMAT      			Coordinate
		FORCE       			off
		MAX         				9999.9999 Truncate
		MIN         				-9999.9999 Truncate
		LEADER      			"B"
		ZERO_FORMAT 		Zero_real
	}
#	ADDRESS cycle_step1 
#	{
#		FORMAT      			Coordinate
#		FORCE       			off
#		MAX         				9999.9999 Truncate
#		MIN         				-9999.9999 Truncate
#		LEADER      			"I"
