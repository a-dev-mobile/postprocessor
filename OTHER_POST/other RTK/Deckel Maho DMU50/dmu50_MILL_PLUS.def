MACHINE DMU50eV

FORMATTING
{
	WORD_SEPARATOR " "
	END_OF_LINE ""
	SEQUENCE Seq_no 1 1 1

	FORMAT  Coordinate "&__5.3_"
	FORMAT  AbsCoord   "%9.4f"
	FORMAT  EventNum   "&__4.0_"
	FORMAT  Dwell_sec  "&__3.1_"
	FORMAT  spindle_code  "%d"
	FORMAT  seq_no     "&__7_00"
	FORMAT  m_code     "%-2d"
	FORMAT  t_code     "%d"
	FORMAT  Feed_IPM   "&__6.4_"
	FORMAT  Feed_IPR   "&__6.4_"
	FORMAT  Feed_MMPM  "&__6.3_"
	FORMAT  Feed_MMPR  "&__6.3_"
	FORMAT  Feed_FRN   "&__7.3_"
	FORMAT  Feed_INV   "&__6.3_"
	FORMAT  Feed       "&__9_00"
	FORMAT  Rev        "%4d"
	FORMAT  Feed_upm   "&__4.1_"
	FORMAT Zero_real "&_01.10"
	FORMAT G_command "%d"
		
	ADDRESS user_add 
	{
		FORMAT      		Coordinate
		FORCE       		off
		MAX         			9999 Truncate
		MIN         			0 Truncate
		LEADER      		"K"
		ZERO_FORMAT 	Zero_real
	}
	ADDRESS cycle_k1 
	{
		FORMAT      		G_command
		FORCE       		off
		MAX         			9999 Truncate
		MIN         			0 Truncate
		LEADER      		"K1="
	}
	ADDRESS Angle 
	{
		FORMAT			Coordinate
		FORCE			off
		MAX				360.0	Warning
		MIN				0		Warning
		ZERO_FORMAT 	Zero_real
		LEADER			"D"
 	}
	ADDRESS N 
	{
		FORMAT			seq_no
		MAX				999998
		MIN				1
		FORCE			off
		LEADER			"N"
	}
#	ADDRESS G 
#	{
#		FORMAT 			g_code
#		MAX    			99
#		MIN    			0
#		FORCE  			always
#	}
	ADDRESS motion_g 
	{
		LEADER 			"G"
		FORMAT 			G_command
		FORCE  			off
	}
	ADDRESS motion_g_cycle
	{
		LEADER 			"G"
		FORMAT 			G_command
		FORCE  			Always
	}
	ADDRESS X  
	{
		FORMAT 			Coordinate
		ZERO_FORMAT 	Zero_real
		FORCE  			off
	}
	ADDRESS Y  
	{
		FORMAT 			Coordinate
		ZERO_FORMAT 	Zero_real
		FORCE  			off
            }
	ADDRESS Z  
	{
		FORMAT 			Coordinate
		ZERO_FORMAT 	Zero_real
		FORCE  			off
	}
	ADDRESS Z_cycle  
	{
		FORMAT 			Coordinate
		ZERO_FORMAT 	Zero_real
		FORCE  			off
		LEADER 			"Z"
	}
	ADDRESS M_clamp4th 
	{
		LEADER 			"M"
		FORMAT 			m_code
		FORCE  			off
	}
	ADDRESS M_clamp5th 
	{
		LEADER 			"M"
		FORMAT 			m_code
		FORCE  			off
	}
	ADDRESS Axis_4th  
	{
		FORMAT 			Coordinate
		ZERO_FORMAT 	Zero_real
		MAX    			5400.001 Warning
		MIN    			-5400.001 Warning 
		FORCE  			off
		LEADER 			[$leader_4th] 
	}
 	ADDRESS Axis_5th
 	{
              	FORMAT 			Coordinate
              	ZERO_FORMAT 	Zero_real
              	MAX    			180.0001 Abort 
              	MIN    			0.000 Abort 
              	FORCE  			off
              	LEADER 			[$leader_5th]
	}
	ADDRESS R 
	{ 
      		LEADER 			"Y"
      		FORMAT 			Coordinate
      		ZERO_FORMAT 	Zero_real
      		FORCE 			off
   	}
 	ADDRESS I  
 	{
              	FORMAT 			Coordinate
              	ZERO_FORMAT 	Zero_real
              	FORCE  			off
	}
 	ADDRESS J  
 	{
              	FORMAT 			Coordinate
              	ZERO_FORMAT 	Zero_real
              	FORCE  			off
	}
 	ADDRESS K  
 	{
              	FORMAT 			Coordinate
              	ZERO_FORMAT 	Zero_real
              	FORCE  			off
	}
 	ADDRESS cycle_dwell  
 	{
              	FORMAT 			Dwell_sec
              	MAX    			900.000 Abort
              	MIN    			0.0 Abort
              	FORCE  			off
              	LEADER 			"X"
	}
	ADDRESS dwell  
	{
		FORMAT 			Dwell_sec
		MAX    			900.000 Abort
		MIN    			0.0 Abort
		FORCE  			off
		LEADER 			"X"
	}
	ADDRESS F  
 	{
		FORMAT 			Feed
		MAX    			999999.9
		MIN    			0.1
		FORCE  			off
	}
	ADDRESS F2_feed
	{
		FORMAT 			Feed
		MAX    			999999.9
		MIN    			0.0
		FORCE  			off
		LEADER			"F2="
	}
	ADDRESS F_tap  
 	{
		FORMAT 			Feed_MMPR
		MAX    			999999.9
		MIN    			0.01
		FORCE  			off
		LEADER 			"J"
	}
	ADDRESS S  
	{
		FORMAT 			spindle_code
		MAX    			30000 Truncate
		MIN    			0
		FORCE  			always
	}
	ADDRESS T  
	{
		FORMAT 			t_code
		MAX    			999999
		MIN    			0
		FORCE  			always
	}
 	ADDRESS coolant_m  
 	{
		LEADER 			"M"
		FORMAT			m_code
		MAX    			99
		MIN    			0
		FORCE  			always
	}
 	ADDRESS spindle_m  
 	{
		LEADER 			"M"
		FORMAT 			m_code
		FORCE  			off
	}
 	ADDRESS M  
 	{
		FORMAT 			m_code
		FORCE  			always
	}
# 	ADDRESS D  
# 	{
#		FORMAT 			d_code
#		MAX    			99
#		MIN    			0
#		FORCE  			off
#	}
	ADDRESS ENUM  
	{
		FORMAT 			seq_no
		FORCE  			always
		LEADER 			""
	}
 	ADDRESS ABSX  
 	{
              	FORMAT 			AbsCoord
              	FORCE  			always
              	LEADER 			""
	}
 	ADDRESS ABSY  
 	{
		FORMAT 			AbsCoord
		FORCE  			always
		LEADER 			""
	}
	ADDRESS ABSZ  
	{
		FORMAT 			AbsCoord
		FORCE  			always
		LEADER 			""
	}
	ADDRESS AXIS4  
	{
		FORMAT 			AbsCoord
		FORCE  			always
		LEADER 			""
	}
 	ADDRESS AXIS5  
 	{
		FORMAT 			AbsCoord
		FORCE  			always
		LEADER 			""
	}
	ADDRESS FEED  
	{
		FORMAT 			Feed
		FORCE  			always
		LEADER 			""
	}
 	ADDRESS RPM  
 	{
              	FORMAT 			Rev
              	FORCE  			always
              	LEADER 			""
	}
   	ADDRESS K3X
  	{
      		FORMAT      			Coordinate
      		FORCE       			off
      		MAX         				99999.9999 Warning
      		MIN         				-99999.9999 Warning
      		ZERO_FORMAT 		Zero_real
      		LEADER 				"X51="
   	}
   	ADDRESS K2X
  	{
      		FORMAT      			Coordinate
      		FORCE       			off
      		MAX         				99999.9999 Warning
