MACHINE  mill

INCLUDE { $UGII_CAM_USER_DEF_EVENT_DIR/ude.cdl }

FORMATTING
{
  WORD_SEPARATOR " "
  END_OF_LINE ""
  SEQUENCE sequence_number 10  10 1 9999

################ FORMAT DECLARATIONS #################
  FORMAT AbsCoord "&__5.4_"
  FORMAT Coordinate "&__4.5_"
  FORMAT Digit_2 "&__2_00"
  FORMAT Digit_4 "&__4_00"
  FORMAT Digit_5 "&__5_00"
  FORMAT Dwell_SECONDS "&__5.3_"
  FORMAT EventNum "&+03_00"
  FORMAT Feed "&__7.2_"
  FORMAT Feed_DPM "&__5.2_"
  FORMAT Feed_FRN "&__5.3_"
  FORMAT Feed_INV "&__5.3_"
  FORMAT Feed_IPM "&__4.1_"
  FORMAT Feed_IPR "&__1.4_"
  FORMAT Hcode "&_02_00"
  FORMAT Rev "&__4_00"
  FORMAT Rotary "&__5.3_"
  FORMAT String "%s"
  FORMAT Tcode "&_02_00"
  FORMAT Zero_int "&_01_0_"
  FORMAT Zero_real "&_01.10"

################ ADDRESS DECLARATIONS ################
  ADDRESS G_cutcom 
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         999999999.9999 Truncate
      MIN         -999999999.9999 Truncate
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_plane 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_adjust 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_feed 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_spin 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_return 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_motion 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_mode 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G 
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
      ZERO_FORMAT Zero_int
  }

  ADDRESS X 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.99999 Truncate
      MIN         -9999.99999 Truncate
      LEADER      [$mom_sys_leader(X)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS Y 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.99999 Truncate
      MIN         -9999.99999 Truncate
      LEADER      [$mom_sys_leader(Y)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS Z 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.99999 Truncate
      MIN         -9999.99999 Truncate
      LEADER      [$mom_sys_leader(Z)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS fourth_axis 
  {
      FORMAT      Rotary
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(fourth_axis)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS fifth_axis 
  {
      FORMAT      Rotary
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(fifth_axis)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS I 
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      ZERO_FORMAT Zero_real
  }

  ADDRESS J 
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      ZERO_FORMAT Zero_real
  }

  ADDRESS K 
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      ZERO_FORMAT Zero_real
  }

  ADDRESS R 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      ZERO_FORMAT Zero_real
  }

  ADDRESS C_axis 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.99999 Truncate
      MIN         -9999.99999 Truncate
      LEADER      [$caxis_dir]
      ZERO_FORMAT Zero_real
  }

  ADDRESS F 
  {
      FORMAT      Feed
      FORCE       off
      MAX         999999.9999 Truncate
      MIN         0.0001 Truncate
      ZERO_FORMAT Zero_real
  }

  ADDRESS S 
  {
      FORMAT      Digit_5
      FORCE       off
      MAX         99999 Truncate
      MIN         0 Truncate
      ZERO_FORMAT Zero_int
  }

  ADDRESS T 
  {
      FORMAT      Tcode
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
  }

  ADDRESS D 
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
      ZERO_FORMAT Zero_int
  }

  ADDRESS H 
  {
      FORMAT      Hcode
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
  }

  ADDRESS dwell 
  {
      FORMAT      Dwell_SECONDS
      FORCE       off
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero_real
  }

  ADDRESS cycle_dwell 
  {
      FORMAT      Dwell_SECONDS
      FORCE       off
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero_real
  }

  ADDRESS K_cycle 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "K"
      ZERO_FORMAT Zero_real
  }

  ADDRESS cycle_step1 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "I"
      ZERO_FORMAT Zero_real
  }

  ADDRESS cycle_step 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "Q"
      ZERO_FORMAT Zero_real
  }

  ADDRESS cycle_orient 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "Q"
      ZERO_FORMAT Zero_real
  }

  ADDRESS P_cutcom 
  {
      FORMAT      Dwell_SECONDS
      FORCE       off
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero_real
  }

  ADDRESS Q_cutcom 
  {
      FORMAT      Dwell_SECONDS
      FORCE       off
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "Q"
      ZERO_FORMAT Zero_real
  }

  ADDRESS E 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      ZERO_FORMAT Zero_real
  }

  ADDRESS M_spindle 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "M"
      ZERO_FORMAT Zero_int
  }

  ADDRESS M_range 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "M"
      ZERO_FORMAT Zero_int
  }

  ADDRESS M_coolant 
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
      LEADER      "M"
      ZERO_FORMAT Zero_int
  }

  ADDRESS M 
  {
      FORMAT      Digit_2
      FORCE       always
      ZERO_FORMAT Zero_int
  }

  ADDRESS end_of_block 
  {
      FORMAT      String
      FORCE       always
      LEADER      ""
  }

  ADDRESS N 
  {
      FORMAT      Digit_4
      FORCE       always
      MAX         9999 Truncate
      LEADER      [$mom_sys_leader(N)]
      ZERO_FORMAT Zero_int
  }

  ADDRESS Text 
  {
      FORMAT      String
      FORCE       always
      LEADER      ""
  }


################ ADDRESS DECLARATIONS ################
  ADDRESS LF_AAXIS 
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_ENUM 
  {
      FORMAT      Digit_5
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_int
  }

  ADDRESS LF_BAXIS 
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_XABS 
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_FEED 
  {
      FORMAT      Feed
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_YABS 
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_SPEED 
  {
      FORMAT      Rev
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_int
  }

  ADDRESS LF_ZABS 
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }


############ BLOCK TEMPLATE DECLARATIONS #############
  BLOCK_TEMPLATE auxfun 
  {
       M[$mom_auxfun]
       end_of_block[#]
  }

  BLOCK_TEMPLATE circular_move 
  {
       G_motion[$mom_sys_circle_code($mom_arc_direction)]
       X[$mom_mcs_goto(0)]
       Y[$mom_mcs_goto(1)]
       I[$mom_arc_center(0)]
       J[$mom_arc_center(1)]
       C_axis[$caxis_angle]
       F[$feed]
       end_of_block[#]
  }

  BLOCK_TEMPLATE coordinate_system 
  {
       G[$mom_sys_reset_code]
  }

  BLOCK_TEMPLATE cutcom_off 
  {
       G_cutcom[$mom_sys_cutcom_code(OFF)]
       end_of_block[#]
  }

  BLOCK_TEMPLATE cutcom_on 
  {
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
       end_of_block[#]
  }

  BLOCK_TEMPLATE cycle_bore 
  {
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_motion[$mom_sys_cycle_bore_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE cycle_bore_back 
  {
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_motion[$mom_sys_cycle_bore_back_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
       cycle_orient[$mom_cycle_orient]\opt
  }

  BLOCK_TEMPLATE cycle_bore_drag 
  {
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_motion[$mom_sys_cycle_bore_drag_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE cycle_bore_dwell 
  {
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_motion[$mom_sys_cycle_bore_dwell_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
       cycle_dwell[$mom_cycle_delay]\opt
  }

  BLOCK_TEMPLATE cycle_bore_manual 
  {
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_motion[$mom_sys_cycle_bore_manual_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE cycle_bore_manual_dwell 
  {
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_motion[$mom_sys_cycle_bore_manual_dwell_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
       cycle_dwell[$mom_cycle_delay]\opt
  }

  BLOCK_TEMPLATE cycle_bore_no_drag 
  {
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_motion[$mom_sys_cycle_bore_no_drag_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
       cycle_orient[$mom_cycle_orient]\opt
  }

  BLOCK_TEMPLATE cycle_drill 
  {
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_motion[$mom_sys_cycle_drill_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE cycle_drill_break_chip 
  {
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_motion[$mom_sys_cycle_drill_break_chip_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
       cycle_step[$mom_cycle_step1]
  }

  BLOCK_TEMPLATE cycle_drill_deep 
  {
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_motion[$mom_sys_cycle_drill_deep_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
       cycle_step[$mom_cycle_step1]
  }

  BLOCK_TEMPLATE cycle_drill_dwell 
  {
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_motion[$mom_sys_cycle_drill_dwell_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
       cycle_dwell[$mom_cycle_delay]\opt
  }

  BLOCK_TEMPLATE cycle_off 
  {
       G_motion[$mom_sys_cycle_off]
  }

  BLOCK_TEMPLATE cycle_tap 
  {
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_motion[$mom_sys_cycle_tap_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE end_of_program 
  {
       M[$mom_sys_end_of_program_code]
       end_of_block[#]
  }

  BLOCK_TEMPLATE fifth_axis_rotate_move 
  {
       G_motion[$mom_sys_rapid_code]
  }

  BLOCK_TEMPLATE fourth_axis_rotate_move 
  {
       G_motion[$mom_sys_rapid_code]
  }

  BLOCK_TEMPLATE incremental_mode 
  {
       G_mode[$mom_sys_output_code(INCREMENTAL)]
  }

  BLOCK_TEMPLATE initial_move 
  {
       G_mode[$mom_sys_output_code($mom_output_mode)]\opt
       X[$mom_pos(0)]
       Y[$mom_pos(1)]
       C_axis[$caxis_angle]
       end_of_block[#]
  }

  BLOCK_TEMPLATE plane_code 
  {
       G_plane[$mom_sys_cutcom_plane_code(XY)]
       end_of_block[#]
  }

  BLOCK_TEMPLATE linear_move 
  {
       G_motion[$mom_sys_linear_code]
       X[$mom_mcs_goto(0)]
       Y[$mom_mcs_goto(1)]
       C_axis[$caxis_angle]
       F[$feed]
       end_of_block[#]
  }

  BLOCK_TEMPLATE opstop 
  {
       M[$mom_sys_optional_stop_code]
       end_of_block[#]
  }

  BLOCK_TEMPLATE output_unit 
  {
       G[$mom_sys_unit_code($mom_output_unit)]
  }

  BLOCK_TEMPLATE prefun 
  {
       G[$mom_prefun]
       end_of_block[#]
  }

  BLOCK_TEMPLATE rapid_traverse 
  {
       X[$mom_pos(0)]
       Y[$mom_pos(1)]
  }

  BLOCK_TEMPLATE reset_c_axis 
  {
       G[$mom_sys_reset_code]
       C_axis[0.0]
       end_of_block[#]
  }

  BLOCK_TEMPLATE rewind_stop_code 
  {
       Text[%]
  }

  BLOCK_TEMPLATE sequence_number 
  {
       N[$mom_seqnum]
  }

  BLOCK_TEMPLATE spindle_range 
  {
       M_range[$mom_sys_spindle_range_code($mom_spindle_range)]\opt
  }

  BLOCK_TEMPLATE stop 
  {
       M[$mom_sys_program_stop_code]
       end_of_block[#]
  }

  BLOCK_TEMPLATE post_cycle_set 
  {
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_motion[$mom_sys_cycle_reps_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE comment_data 
  {
       LF_XABS[$mom_pos(0)]
       LF_YABS[$mom_pos(1)]
       LF_ZABS[$mom_pos(2)]
       LF_AAXIS[$mom_pos(3)]
       LF_BAXIS[$mom_pos(4)]
       LF_FEED[$mom_feed_rate]
       LF_SPEED[$mom_spindle_speed]
  }

}
