MACHINE mill3ax
 
INCLUDE  { $UGII_CAM_USER_DEF_EVENT_DIR/ude.cdl }

FORMATTING
{
  WORD_SEPARATOR " "
  END_OF_LINE ""
  SEQUENCE sequence_number 1  1 1 99

################ FORMAT DECLARATIONS #################
  FORMAT AbsCoord "&__5.4_"
  FORMAT Coordinate "&__3.3_"
  FORMAT Digit_2 "&_02_00"
  FORMAT Digit_4 "&_04_00"
  FORMAT Digit_5 "&__5_00"
  FORMAT Dwell_SECONDS "&__5.3_"
  FORMAT Dwell_sec "&__5.3_"
  FORMAT EventNum "&+03_00"
  FORMAT Feed "&__7.1_"
  FORMAT Feed_DPM "&__5.2_"
  FORMAT Feed_FRN "&__4.1_"
  FORMAT Feed_INV "&__5.1_"
  FORMAT Feed_IPM "&__4.1_"
  FORMAT Feed_IPR "&__5.1_"
  FORMAT Feed_MMPM "&__5.0_"
  FORMAT Feed_MMPR "&__5.2_"
  FORMAT Hcode "&_02_00"
  FORMAT Rev "&__4_00"
  FORMAT Rotary "&__5.3_"
  FORMAT String "%s"
  FORMAT Tcode "&_02_00"
  FORMAT Zero "&_01.10"
  FORMAT Zero_int "&_02_00"

################ ADDRESS DECLARATIONS ################
  ADDRESS G_cutcom 
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         999999999.9999 Truncate
      MIN         -999999999.9999 Truncate
      LEADER      "G"
  }

  ADDRESS G_plane 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
  }

  ADDRESS G_adjust 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
  }

  ADDRESS G_feed 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
  }

  ADDRESS G_spin 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
  }

  ADDRESS G_return 
  {
      FORMAT      Digit_2
      FORCE       always
      LEADER      "G"
  }

  ADDRESS G_motion 
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         9999 Truncate
      MIN         -9999 Truncate
      LEADER      "G"
  }

  ADDRESS G_mode 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
  }

  ADDRESS G 
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
  }

  ADDRESS X 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(X)]
      ZERO_FORMAT Zero
  }

  ADDRESS Y 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(Y)]
      ZERO_FORMAT Zero
  }

  ADDRESS Z 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(Z)]
      ZERO_FORMAT Zero
  }

  ADDRESS fourth_axis 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(fourth_axis)]
      ZERO_FORMAT Zero
  }

  ADDRESS fifth_axis 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(fifth_axis)]
      ZERO_FORMAT Zero
  }

  ADDRESS I 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      ZERO_FORMAT Zero
  }

  ADDRESS J 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      ZERO_FORMAT Zero
  }

  ADDRESS K 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      ZERO_FORMAT Zero
  }

  ADDRESS R 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      ZERO_FORMAT Zero
  }

  ADDRESS F 
  {
      FORMAT      Feed
      FORCE       off
      MAX         999999.9999 Truncate
      MIN         0.0001 Truncate
      ZERO_FORMAT Zero
  }

  ADDRESS S 
  {
      FORMAT      Digit_5
      FORCE       off
      MAX         99999 Truncate
      MIN         0 Truncate
      ZERO_FORMAT Zero_int
  }

  ADDRESS T 
  {
      FORMAT      Tcode
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
  }

  ADDRESS D 
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
  }

  ADDRESS H 
  {
      FORMAT      Hcode
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
  }

  ADDRESS dwell 
  {
      FORMAT      Dwell_sec
      FORCE       off
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "F"
      ZERO_FORMAT Zero
  }

  ADDRESS cycle_dwell 
  {
      FORMAT      Dwell_sec
      FORCE       off
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero
  }

  ADDRESS P_cutcom 
  {
      FORMAT      Dwell_sec
      FORCE       off
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero
  }

  ADDRESS K_cycle 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "K"
      ZERO_FORMAT Zero
  }

  ADDRESS cycle_step1 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "I"
      ZERO_FORMAT Zero
  }

  ADDRESS cycle_orient 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "Q"
      ZERO_FORMAT Zero
  }

  ADDRESS cycle_step 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "Q"
      ZERO_FORMAT Zero
  }

  ADDRESS Q_cutcom 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "Q"
      ZERO_FORMAT Zero
  }

  ADDRESS E 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      ZERO_FORMAT Zero
  }

  ADDRESS M_spindle 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "M"
  }

  ADDRESS M_coolant 
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
      LEADER      "M"
  }

  ADDRESS M_range 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "M"
  }

  ADDRESS M 
  {
      FORMAT      Digit_2
      FORCE       off
  }

  ADDRESS N 
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      [$mom_sys_leader(N)]
  }

  ADDRESS Text 
  {
      FORMAT      String
      LEADER      ""
  }


################ ADDRESS DECLARATIONS ################
  ADDRESS LF_AAXIS 
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero
  }

  ADDRESS LF_ENUM 
  {
      FORMAT      Digit_5
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_int
  }

  ADDRESS LF_BAXIS 
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero
  }

  ADDRESS LF_XABS 
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero
  }

  ADDRESS LF_FEED 
  {
      FORMAT      Feed
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero
  }

  ADDRESS LF_YABS 
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero
  }

  ADDRESS LF_SPEED 
  {
      FORMAT      Rev
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_int
  }

  ADDRESS LF_ZABS 
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero
  }


############ BLOCK TEMPLATE DECLARATIONS #############
  BLOCK_TEMPLATE absolute_mode 
  {
       G_cutcom[$mom_sys_cutcom_code(OFF)]
       G_mode[$mom_sys_output_code(ABSOLUTE)]
       G[$mom_sys_unit_code(IN)]
  }

  BLOCK_TEMPLATE approach_move 
  {
       M[11]
  }

  BLOCK_TEMPLATE auxfun 
  {
       M[$mom_auxfun]
  }

  BLOCK_TEMPLATE cancel_cutcom 
  {
       G_cutcom[$mom_sys_cutcom_code(OFF)]
  }

  BLOCK_TEMPLATE circular_move 
  {
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
       G_motion[$mom_sys_circle_code($mom_arc_direction)]\opt
       G_mode[$mom_sys_output_code($mom_output_mode)]
       X[$mom_pos(0)]
       Y[$mom_pos(1)]
       Z[$mom_pos(2)]
       I[$mom_prev_pos(0) - $mom_pos_arc_center(0)]
       J[$mom_prev_pos(1) - $mom_pos_arc_center(1)]
       K[$mom_prev_pos(2) - $mom_pos_arc_center(2)]
       F[$feed]
  }

  BLOCK_TEMPLATE circular_move_1 
  {
       G_motion[$mom_sys_circle_code($mom_arc_direction)]\opt
       G_mode[$mom_sys_output_code(ABSOLUTE)]
       X[$mom_pos(0)]
       Y[$mom_pos(1)]
       I[$mom_pos_arc_center(0)]
       J[$mom_pos_arc_center(1)]
       F[$feed]
  }

  BLOCK_TEMPLATE circular_move_turbo 
  {
       G_cutcom[^sys_cutcom_code(CUTCOM_STATUS)]\opt
       G_motion[^sys_circle_code(ARC_DIRECTION)]\opt
       G_mode[^sys_output_code(OUTPUT_MODE)]
       X[^POSX]
       Y[^POSY]
       Z[^POSZ]
       I[^CENTERX]
       J[^CENTERY]
       K[^CENTERZ]
       F[^FEED]
       M_spindle[^sys_spindle_direction_code(SPINDLE_DIRECTION)]\opt
       M_coolant[^sys_coolant_code(COOLNT_STATUS)]\opt
  }

  BLOCK_TEMPLATE coolant_off 
  {
       M_coolant[$mom_sys_coolant_code(OFF)]
  }

  BLOCK_TEMPLATE coordinate_system 
  {
       G[$mom_sys_reset_code]
       X[0.0]
       Y[0.0]
  }

  BLOCK_TEMPLATE cutcom_off 
  {
       G_cutcom[$mom_sys_cutcom_code(OFF)]
  }

  BLOCK_TEMPLATE cutcom_on 
  {
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
  }

  BLOCK_TEMPLATE cycle_bore 
  {
       G_motion[$mom_sys_cycle_bore_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE cycle_bore_back 
  {
       G_motion[$mom_sys_cycle_bore_back_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE cycle_bore_drag 
  {
       G_motion[$mom_sys_cycle_bore_drag_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE cycle_bore_dwell 
  {
       G_motion[$mom_sys_cycle_bore_dwell_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
       cycle_dwell[$mom_cycle_delay]\opt
  }

  BLOCK_TEMPLATE cycle_bore_manual_dwell 
  {
       G_motion[$mom_sys_cycle_bore_manual_dwell_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
       cycle_dwell[$mom_cycle_delay]\opt
  }

  BLOCK_TEMPLATE cycle_bore_manual 
  {
       G_motion[$mom_sys_cycle_bore_manual_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE cycle_bore_no_drag 
  {
       G_motion[$mom_sys_cycle_bore_no_drag_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE cycle_drill_break_chip 
  {
       G_motion[$mom_sys_cycle_drill_break_chip_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
       cycle_step[$mom_cycle_step1]
  }

  BLOCK_TEMPLATE cycle_drill 
  {
       G_motion[$mom_sys_cycle_drill_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE cycle_drill_deep 
  {
       G_motion[$mom_sys_cycle_drill_deep_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
       cycle_step[$mom_cycle_step1]
  }

  BLOCK_TEMPLATE cycle_drill_dwell 
  {
       G_motion[$mom_sys_cycle_drill_dwell_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
       cycle_dwell[$mom_cycle_delay]\opt
  }

  BLOCK_TEMPLATE cycle_off 
  {
       G_motion[$mom_sys_cycle_off]
  }

  BLOCK_TEMPLATE cycle_tap 
  {
       G_motion[$mom_sys_cycle_tap_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE delay 
  {
       G[$mom_sys_delay_code($mom_delay_mode)]
       dwell[$mom_delay_value]
  }

  BLOCK_TEMPLATE dwell 
  {
       G[4]
       dwell[$mom_delay_value]
  }

  BLOCK_TEMPLATE end_of_path 
  {
       M_spindle[$mom_sys_spindle_direction_code(OFF)]
       M_coolant[$mom_sys_coolant_code(OFF)]
  }

  BLOCK_TEMPLATE end_of_program 
  {
       M[$mom_sys_end_of_program_code]
  }

  BLOCK_TEMPLATE engage_move 
  {
       M[58]
  }

  BLOCK_TEMPLATE from 
  {
       G_motion[$mom_sys_rapid_code]
       X[$mom_from_pos(0)]
       Y[$mom_from_pos(1)]
  }

  BLOCK_TEMPLATE inch_metric_mode 
  {
       G[$mom_sys_unit_code(IN)]
  }

  BLOCK_TEMPLATE incremental_mode 
  {
       G_mode[$mom_sys_output_code(INCREMENTAL)]
  }

  BLOCK_TEMPLATE knife_off 
  {
       M_spindle[59]
  }

  BLOCK_TEMPLATE knife_on 
  {
       M_spindle[58]
  }

  BLOCK_TEMPLATE linear_move 
  {
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
       G_motion[$mom_sys_linear_code]
       G_mode[$mom_sys_output_code($mom_output_mode)]
       X[$mom_pos(0)]
       Y[$mom_pos(1)]
       Z[$mom_pos(2)]
       F[$feed]
       D[$mom_cutcom_adjust_register]\opt
       M_spindle[$mom_sys_spindle_direction_code($mom_spindle_direction)]\opt
       M_coolant[$mom_sys_coolant_code(ON)]\opt
  }

  BLOCK_TEMPLATE linear_move_1 
  {
       G_motion[$mom_sys_linear_code]
       G_mode[$mom_sys_output_code(ABSOLUTE)]
       X[$mom_pos(0)]
       Y[$mom_pos(1)]
       F[$feed]
  }

  BLOCK_TEMPLATE linear_move_turbo 
  {
       G_cutcom[^sys_cutcom_code(CUTCOM_STATUS)]\opt
       G_motion[^sys_linear_code]
       G_mode[^sys_output_code(OUTPUT_MODE)]
       X[^POSX]
       Y[^POSY]
       Z[^POSZ]
       F[^FEED]
       D[^cutcom_adjust_register]\opt
       M_spindle[^sys_spindle_direction_code(SPINDLE_DIRECTION)]\opt
       M_coolant[^sys_coolant_code(COOLNT_STATUS)]\opt
  }

  BLOCK_TEMPLATE manual_tool_change 
  {
       M[$mom_sys_program_stop_code]
  }

  BLOCK_TEMPLATE opstop 
  {
       M[$mom_sys_optional_stop_code]\opt\nows
  }

  BLOCK_TEMPLATE output_unit 
  {
       G[$mom_sys_unit_code(IN)]
  }

  BLOCK_TEMPLATE park_xy 
  {
       G_motion[$mom_sys_rapid_code]
       X[$mom_sys_home_pos(0)]
       Y[$mom_sys_home_pos(1)]
  }

  BLOCK_TEMPLATE prefun 
  {
       G[$mom_prefun]
  }

  BLOCK_TEMPLATE rapid_move_turbo 
  {
       G_motion[^sys_rapid_code]
       G_mode[^sys_output_code(OUTPUT_MODE)]
       X[^POSX]
       Y[^POSY]
       Z[^POSZ]
       H[^tool_adjust_register]\opt
       M_spindle[^sys_spindle_direction_code(SPINDLE_DIRECTION)]\opt
       M_coolant[^sys_coolant_code(COOLNT_STATUS)]\opt
  }

  BLOCK_TEMPLATE rapid_spindle 
  {
       G_motion[$mom_sys_rapid_code]
       G_mode[$mom_sys_output_code($mom_output_mode)]\opt
       X[$mom_pos(0)]
       Y[$mom_pos(1)]
       Z[$mom_pos(2)]
       H[$mom_tool_adjust_register]\opt
  }

  BLOCK_TEMPLATE rapid_traverse 
  {
       G_motion[$mom_sys_rapid_code]
       G_mode[$mom_sys_output_code($mom_output_mode)]\opt
       X[$mom_pos(0)]
       Y[$mom_pos(1)]
       Z[$mom_pos(2)]
  }

  BLOCK_TEMPLATE return_home 
  {
       G_mode[91]
  }

  BLOCK_TEMPLATE return_motion 
  {
       M_spindle[$mom_sys_spindle_direction_code(OFF)]
       M_coolant[$mom_sys_coolant_code(OFF)]
  }

  BLOCK_TEMPLATE return_motion_1 
  {
       G_mode[91]
       T[$mom_tool_number]
       H[$mom_sys_zero]
  }

  BLOCK_TEMPLATE return_motion_2 
  {
       M_spindle[$mom_sys_spindle_direction_code(OFF)]
       M_coolant[$mom_sys_coolant_code(OFF)]
  }

  BLOCK_TEMPLATE return_motion_5 
  {
       G_mode[91]
       H[$mom_sys_zero]
  }

  BLOCK_TEMPLATE return_move 
  {
       M[25]
  }

  BLOCK_TEMPLATE return_move_1 
  {
       M[59]
  }

  BLOCK_TEMPLATE rewind_stop_code 
  {
       Text[%]
  }

  BLOCK_TEMPLATE sequence_number 
  {
       N[$mom_seqnum]
  }

  BLOCK_TEMPLATE sequence_number_turbo 
  {
       N[^SEQNUM]
  }

  BLOCK_TEMPLATE set_zero 
  {
       G_motion[98]
       X[0]
       Y[0]
  }

  BLOCK_TEMPLATE spindle_off 
  {
       M_spindle[$mom_sys_spindle_direction_code(OFF)]
  }

  BLOCK_TEMPLATE spindle_range 
  {
       M_range[$mom_sys_spindle_range_code($mom_spindle_range)]\opt
  }

  BLOCK_TEMPLATE spindle_rpm 
  {
       M_spindle[$mom_sys_spindle_direction_code($mom_spindle_direction)]\opt
  }

  BLOCK_TEMPLATE spindle_rpm_1 
  {
       M_spindle[$mom_sys_spindle_direction_code(CLW)]\opt
  }

  BLOCK_TEMPLATE start_of_program 
  {
       G_mode[$mom_sys_output_code(ABSOLUTE)]
  }

  BLOCK_TEMPLATE start_of_program_1 
  {
       G_motion[$mom_sys_rapid_code]
  }

  BLOCK_TEMPLATE stop 
  {
       M[$mom_sys_program_stop_code]
  }

  BLOCK_TEMPLATE tool_change 
  {
       G_mode[$mom_sys_output_code(INCREMENTAL)]
       G[$mom_sys_return_code]
       Z[0]
  }

  BLOCK_TEMPLATE tool_change_1 
  {
       T[$mom_tool_number]
       M[$mom_sys_tool_change_code]
  }

  BLOCK_TEMPLATE tool_change_2 
  {
       T[$mom_next_tool_number]
  }

  BLOCK_TEMPLATE tool_len_adj_off 
  {
       H[$mom_sys_zero]
  }

  BLOCK_TEMPLATE tool_length_adjust 
  {
       H[$mom_tool_adjust_register]
  }

  BLOCK_TEMPLATE tool_preselect 
  {
       T[$mom_next_tool_number]
  }

  BLOCK_TEMPLATE post_cycle_set 
  {
       G_motion[$mom_sys_cycle_reps_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE comment_data 
  {
       LF_XABS[$mom_pos(0)]
       LF_YABS[$mom_pos(1)]
       LF_FEED[$mom_feed_rate]
       LF_SPEED[$mom_spindle_speed]
  }

}
